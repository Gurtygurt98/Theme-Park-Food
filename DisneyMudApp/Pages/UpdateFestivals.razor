@page "/UpdateFestivals"
@using DisneyMudApp
@using MudBlazor
@using SQL_API
<MudText Typo="Typo.h1">Festival Form</MudText>
<MudTextField @bind-Value="FestivalName" Label="Festival Name" Variant="Variant.Outlined"></MudTextField>
<MudTextField @bind-Value="Description" Label="Description" Variant="Variant.Outlined" Lines="3"></MudTextField>

@*Change to a drop down with autofill options https://mudblazor.com/components/autocomplete#strict-mode *@
@code {
    string FestivalName;
    string Description;
    string park;
}
